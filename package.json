{
  "name": "apishopper",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ignore-pattern 'node_modules/**/*' --ext .ts",
    "lint:fix": "eslint . --ignore-pattern 'node_modules/**/*' --ext .ts --fix",
    "prisma:generate": "npx prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=src/db/schema.prisma --name init",
    "prisma:seed": "ts-node src/db/seeds.ts",
    "docker": "npm install && npx prisma generate --schema=prisma/schema.prisma && npx prisma migrate dev --schema=prisma/schema.prisma --name init && nodemon src/index.ts",
    "test": "mocha -r ts-node/register src/tests/**/**/*.test.ts --exit -t 60000 --exit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/chai": "^4.3.12",
    "@types/chai-as-promised": "^7.1.8",
    "@types/chai-http": "4.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mocha": "9.1.0",
    "@types/node": "^22.5.0",
    "@types/sinon": "10.0.11",
    "@types/sinon-chai": "3.2.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-http": "4.3.0",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "mocha": "^9.2.2",
    "nodemon": "^3.1.4",
    "prisma": "^5.19.0",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "@prisma/client": "^5.19.0",
    "arg": "^5.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
